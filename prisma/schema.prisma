generator client {
  provider = "prisma-client-js"
  // Remove or comment out any custom output path
  // output = "./generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://user:password@https://knihobudka-online-production.up.railway.app/:5432/booksdb?schema=public"
}
model books {
  bookId     Int       @id @default(autoincrement())
  surname    String
  name       String
  title      String
  year       String
  isbn       String
  deleted    Boolean   @default(false)
}
